    "dev:oneplus": "cross-env CHOOSE_VENDER=oneplus yarn dev",
    "serve": "yarn run babel ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --progress --config build/webpack.dev.js",
    "build": "yarn run _build --mode production",
    "build:prod": "yarn run build --env.AEM",
    "build:patch": "yarn run build:prod --env.PATCH",
    "build:dev": "yarn run _build --mode development",
    "start": "yarn run dev --open",
    "_build": "cross-env NODE_ENV=production yarn run babel ./node_modules/webpack/bin/webpack.js --progress --config build/webpack.config.js",
    "babel": "node --stack-size=65535 -r @babel/register",
    "build:font": "webpack --mode production --config ./build/font-spider/webpack.font.babel.js",
    "font-spider": "yarn run build:font && node ./build/font-spider/index.js",
    "lint": "eslint --ext .js,.ts src/",
    "check:types": "tsc",
    "declare:envs": "yarn babel scripts/gen-env-declarations.js --page",
    "browserslist": "browserslist",
    "release": "standard-version"
