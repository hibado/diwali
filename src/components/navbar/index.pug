        font-family: OPPOSans-Ver2-Regular;
        font-weight: 550;
      }

    .oppo-navbar
      nav#navbar(data-theme=theme data-theme-default=theme)
        .container
          .mask.g--ls-hidden
          // mb端产品tab
          ul.selector.product-selector.g--ls-hidden(data-selector='link')
            li.selected
              a A91 Pro
            li.indicator
