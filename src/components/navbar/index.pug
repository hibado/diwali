          // mb端模块tab
          ul.selector.link-selector.g--ls-hidden(data-selector='link')
            li.selected
              a Overview
            li.indicator
              svg.arrow(width='14px' height='14px' viewbox='0 0 14 14' version='1.1' xmlns='http://www.w3.org/2000/svg' xmlns:xlink='http://www.w3.org/1999/xlink')
                g
                  path(d='M4.29289322,10.2928932 C3.90236893,10.6834175 3.90236893,11.3165825 4.29289322,11.7071068 C4.68341751,12.0976311 5.31658249,12.0976311 5.70710678,11.7071068 L10.4142136,7 L5.70710678,2.29289322 C5.31658249,1.90236893 4.68341751,1.90236893 4.29289322,2.29289322 C3.90236893,2.68341751 3.90236893,3.31658249 4.29289322,3.70710678 L7.58578644,7 L4.29289322,10.2928932 Z')
          // 产品tab
          ul.products
            each product, i in products
              if (i > 0)
                li.divider
                  span |
              li.product
                a(
                  class=parseClasses({ active: product.active })
                  href=(product.active && "javascript:;" || "https://www.oppo.com/")
                  )!= product.name
          // 模块tab
          .linkwrapper
            ul.links.module
              li.link.pc.hightlight
                a(href='#') Home
              each link, i in links
                li.link.post
                  if (link.anchor)
                    a(href=link.anchor data-link=link.anchor)!= link.name
                  else
                    a(href=(link.href || 'https://www.oppo.com/'))!= link.name
          ul.links.page
            li.link.page
              a(href='www.oppo.com') Specs
            li.link.page
              a(href='#') Compare
            li.link.page
              a(href='#') 3D Experience
          // 购买按钮 开始
          ul.buttons(data-theme='green')
            li.button.buy
              a(href='#link.button-buy') !{__ZH__ ? '立即购买' : 'Buy Now'}
